{
  "name": "n8n-nodes-cheerio-html-parser",
  "version": "0.5.0",
  "description": "n8n node to parse HTML using Cheerio",
  "keywords": [
    "n8n",
    "node",
    "cheerio",
    "html",
    "parser"
  ],
  "license": "MIT",
  "homepage": "https://github.com/zzzarius/n8n-nodes-cheerio-html-parser",
  "author": {
    "name": "Darius Zivertas",
    "email": "n8n-nodes-cheerio-html-parser@zivertas.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zzzarius/n8n-nodes-cheerio-html-parser"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "lint:dangerous": "biome lint --write --unsafe .",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/CheerioHTMLParser/CheerioHTMLParser.node.js"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@types/jest": "^30.0.0",
    "gulp": "^5.0.1",
    "jest": "^30.0.0",
    "n8n-workflow": "~1.82.0",
    "ts-jest": "^29.3.4",
    "typescript": "~5.9.2"
  },
  "dependencies": {
    "cheerio": "1.1.2",
    "n8n-core": "~1.106.1"
  }
}
